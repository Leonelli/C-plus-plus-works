using namespace std;
#include <iostream>

// stampa i numeri da m a n
int stampa (int m, int n) { // m,m+1,...,n-1,n
  if (m>n)  
    cout << endl;
  else { 
    cout << m << " ";
    stampa(m+1,n);
  }
} 


int main() 
{
  int n;
  cout << "m? n? (n,m>=0)";
  cin >> m >> n;
  cout << "stampanumeri(" << m << ","  << n << ") = \n";
  stampa(m,n);
return 0;
}

